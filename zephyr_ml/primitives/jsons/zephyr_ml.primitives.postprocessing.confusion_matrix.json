{
    "name": "zephyr_ml.primitives.postprocessing.confusion_matrix",
    "contributors": [
        "Raymond Pan <rpan@mit.edu>"
    ],
    "description": "Plot confusion matrix.",
    "classifiers": {
        "type": "helper"
    },
    "modalities": [],
    "primitive": "zephyr_ml.primitives.postprocessing.confusion_matrix",
    "produce": {
        "args": [
            {
                "name": "y_true",
                "type": "ndarray"
            },
            {
                "name": "y_pred",
                "type": "ndarray"
            }
        ],
        "output": [
            {
                "name": "confusion_matrix",
                "type": "ndarray"
            }
        ]

    },
   
    "hyperparameters": {
        "fixed": {
            "labels": {
                "type": "ndarray",
                "default": null
            },
            "sample_weight": {
                "type": "ndarray",
                "default": null
            },
            "normalize": {
                "type": "str",
                "default": null
            },
            "show_plot": {
                "type": "bool",
                "default": true
            }
        }
    }
    
}